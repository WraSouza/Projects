<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="Projects.Views.DetailsProjectPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:mvvm="clr-namespace:Projects.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="DetailsProjectPage"
    x:DataType="mvvm:DetailsProjectViewModel">

    <StackLayout>

        <Label Text="{Binding ProjectName}"/>

        <Label Text="{Binding DeadLine}"/>

        <Label Text="{Binding Owner}"/>



    </StackLayout>
</ContentPage>